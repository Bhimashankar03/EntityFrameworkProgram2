@model IEnumerable<Employee>

	<table class="table">
<thead>
	<tr>
			<th>EmpId</th>
			<th>EmpName</th>
			<th>Address</th>
			<th>EmpMail</th>
		    <th>Action</th>
			<th>Action</th>

	</tr>
</thead>
<tbody>
	@{
		foreach(var employee in Model)
		{
		<tr>
				<td>@employee.EmpId</td>
				<td>@employee.EmpName</td>
				<td>@employee.Address</td>
				<td>@employee.EmpMail</td>
				<td><a asp-action="Delete" asp-controller="Employee" asp-route-id=@employee.EmpId>Delete</a></td>
				<td><a asp-action="Update" asp-controller="Employee" asp-route-id=@employee.EmpId>Update</a></td>
		</tr>
		}
		}
</tbody>
	</table>

	<a href="Employee/create">Add new Emp</a>